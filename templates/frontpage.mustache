                        <a href="{{{config.wwwroot}}}/course/index.php?categoryid=5">
                            <div class="cat-cta">
                                <div class="icon">
                                    <img src="{{{i-our_FA}}}">
                                </div>
                                <div class="title_cat">{{#str}}CenterforLegalResponses, theme_fos_space1{{/str}}</div>
                            </div>
