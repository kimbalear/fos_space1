                            <div class="title">{{#str}}slider2text, theme_fos_space1{{/str}}</div>
                        </div>
                        <div class="item">
                            <img src="{{{slider3}}}" class="image-desktop">
                            <div class="title">{{#str}}slider3text, theme_fos_space1{{/str}}</div>
                        </div>
                    </div>
                </div>

                {{#secondarymoremenu}}
